{
  "name": "thesis",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:hrnyc-targaryen/thesis.git",
  "author": "Alex Chojnacki <thealex@umich.edu>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn run dev:server\" \"yarn run dev:client\"",
    "dev:client": "cd client && yarn run start",
    "dev:server": "nodemon server.js",
    "install": "yarn run install:client",
    "install:client": "pwd && ls && cd client && yarn install && yarn build",
    "start": "node server.js",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "grommet": "^1.10.1",
    "history": "^4.7.2",
    "pg": "^7.4.1",
    "pg-format": "^1.0.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1"
  }
}
