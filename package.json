    "express-rate-limit": "^2.11.0",
    "fs": "^0.0.1-security",
    "grommet": "^1.10.1",
    "history": "^4.7.2",
    "html-minifier": "^3.5.12",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "pg": "^7.4.1",
    "pg-format": "^1.0.4",
    "react-c3js": "^0.1.20",
    "react-dropzone": "^4.2.9",
    "react-email-editor": "^0.5.0",
